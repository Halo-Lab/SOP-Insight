{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node --import ./dist/sentry-init.js ./dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm --import ./sentry-init.ts ./src/server.ts'",
    "build": "tsc && cp .env ./dist/.env 2>/dev/null || : && cp sentry-init.ts ./dist/sentry-init.ts 2>/dev/null || : && cp sentry.ts ./dist/sentry.ts 2>/dev/null || :",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@sentry/node": "^9.18.0",
    "@sentry/profiling-node": "^9.18.0",
    "@supabase/supabase-js": "^2.39.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "openai": "^4.98.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}